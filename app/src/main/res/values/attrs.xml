<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="CustomAuthenticationButton">
        <attr name="auth_btn_enable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomSecondaryButton">
        <attr name="secondary_button_state" format="enum">
            <enum name="DISABLE" value="0"/>
            <enum name="BLUE" value="1"/>
            <enum name="YELLOW" value="2"/>
            <enum name="GREEN" value="3"/>
            <enum name="WHITE_WITH_BLUE_BORDER" value="4"/>
            <enum name="BLACK_WITH_BLUE_BORDER" value="5"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomPhoneNoEditText">
        <attr name="phone_no_initial_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3"/>
        </attr>
        <attr name="phone_no_error_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3"/>
        </attr>
        <attr name="phone_no_success_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3"/>
        </attr>
        <attr name="phone_no_hint" format="string" />
        <attr name="phone_no_error_text" format="string" />


    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="edit_initial_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3"/>
        </attr>
        <attr name="edit_error_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3"/>
        </attr>
        <attr name="edit_success_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3" />
        </attr>

        <attr name="edit_title" format="string" />
        <attr name="edit_info" format="string" />
        <attr name="edit_hint" format="string" />
        <attr name="edit_error_text" format="string" />
        <attr name="android:src" />
    </declare-styleable>

    <declare-styleable name="CustomImageTextButton">
        <attr name="citb_src" format="reference" />
        <attr name="citb_btn_title" format="string" />
        <attr name="citb_btn_type" format="enum">
            <enum name="IMAGE" value="0" />
            <enum name="NO_IMAGE_GREY" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomSpinner">
        <attr name="cs_initial_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3" />
        </attr>
        <attr name="cs_success_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3" />
        </attr>
        <attr name="cs_error_state" format="enum">
            <enum name="GRAY" value="0"/>
            <enum name="GREEN" value="1"/>
            <enum name="RED" value="2"/>
            <enum name="BLUE" value="3" />
        </attr>
        <attr name="cs_title" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomFelloView">
        <attr name="cfv_type" format="enum">
            <enum name="FOLLOWERS" value="0" />
            <enum name="FOLLOWING" value="1" />
            <enum name="SOLD" value="2" />
        </attr>
        <attr name="cfv_count" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomCollectionChartsView">
        <attr name="cccv_type" format="enum">
            <enum name="MARKET_VALUE" value="0" />
            <enum name="COLLECTION_SIZE" value="1" />
            <enum name="AVERAGE_PRICE" value="2" />
        </attr>
        <attr name="cccv_amount" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomCollectionButton">
        <attr name="ccb_type" format="enum">
            <enum name="COLLECTION" value="0"/>
            <enum name="FAVORITES" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomDeliveryButton">
        <attr name="cdb_type" format="enum">
            <enum name="PRICE" value="0"/>
            <enum name="PENDING" value="1"/>
            <enum name="TRANSIT" value="2"/>
            <enum name="DELIVERED" value="3"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="CustomCollectionTypeView">
        <attr name="ccv_isActive" format="boolean"/>
        <attr name="ccv_isUpdate" format="boolean"/>
        <attr name="cccv_text" format="string" />
    </declare-styleable>


    <declare-styleable name="CustomFilterCategoryView">
        <attr name="cfcv_isActive" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomFilterSubCategoryView">
        <attr name="cfscv_isActive" format="boolean"/>
    </declare-styleable>


</resources>